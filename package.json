        "@react-navigation/native-stack": "^7.1.10",
        "axios": "^1.7.9",
        "i18next": "^24.0.2",
        "immer": "^10.1.1",
        "react": "18.3.1",
        "react-i18next": "^15.1.3",
        "react-native": "0.76.3",
        "react-native-localize": "^3.3.0",
