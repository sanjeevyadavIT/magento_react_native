{
    "name": "MageCart",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "set:dev": "shx echo \"const environment: 'staging' | 'production' = 'staging';\n\nexport default environment;\"> config/active.env.ts",
        "set:prod": "shx echo \"const environment: 'staging' | 'production' = 'production';\n\nexport default environment;\"> config/active.env.ts",
        "start": "react-native start",
        "start:dev": "npm run set:dev; npm start",
        "start:prod": "npm run set:prod; npm start",
        "test": "jest"
    },
